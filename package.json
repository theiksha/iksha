{
	"name": "iksha",
	"homepage": "https://theiksha.github.io/iksha",
	"version": "0.0.2",
	"private": true,
	"scripts": {
		"predeploy": "npm run build",
		"deploy": "gh-pages -d build",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
